@unexec if cmp -s %D/etc/emailrelay.conf.template %D/etc/emailrelay.conf; then rm -f %D/etc/emailrelay.conf; fi
etc/emailrelay.conf.template
@exec if [ ! -f %D/etc/emailrelay.conf ] ; then cp -p %D/%F %B/emailrelay.conf; fi
